  
<h1>Check out all these songs!</h1>
<table>
  <thead>
    <tr>
      <th>Song Title</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th>Artist Name</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
        <td>
          <%= link_to 'View', song %> |
          <%= link_to 'Edit', edit_song_path(song) %> |
          <%= link_to 'Delete', song, method: :delete, data: { confirm: 'For real?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
  
</table>
<br><%= link_to "Create a new song", new_song_path %>