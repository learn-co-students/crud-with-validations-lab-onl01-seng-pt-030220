<%= form_for(@song) do |f|%>


    <div class="field">
        <%= f.text_field :title %>
    </div>

    <div class="field">
        <%= f.check_box :released %>
    </div>

    <div class="field">
        <%= f.number_field :release_year %>
    </div>

    <div class="field">
        <%= f.text_field :genre %>
    </div>

    <div class="field">
        <%= f.text_field :artist_name %>
    </div>

    <div class="actions">
        <%= f.submit %>
    </div>
<%end%>
